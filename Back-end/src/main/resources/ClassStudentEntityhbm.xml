<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.ex.Models.ClassStudentEntity" table="class_student" schema="public" catalog="postgres">
        <id name="pairId">
            <column name="pair_id" sql-type="integer"/>
        </id>
        <property name="classId">
            <column name="class_id" sql-type="integer" not-null="true"/>
        </property>
        <property name="studentId">
            <column name="student_id" sql-type="integer" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>